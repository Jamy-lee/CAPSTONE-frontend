    console.log(this.name, this.email, this.contact, this.message);\n      fetch(\"...\", {\n        method: \"POST\",\n        body: JSON.stringify({\n          name: this.name,\n          email: this.email,\n          number: this.number,\n          message: this.message,\n        }),\n        headers: {\n          \"Content-type\": \"application/json; charset=UTF-8\",\n        },\n      })\n        .then((response) => response.json())\n        .then((json) => {\n          console.log(json);\n          alert(\"Message sent successfully\");\n          (this.name = \"\"),\n            (this.email = \"\"),\n            (this.number = \"\"),\n            (this.message = \"\");\n        });\n    },\n  },\n};\n</script>\n\n<style>\n#contact {\n  height: 100vh;\n  padding-top: 120px;\n}\ninput {\n  height: 350px;\n  border: none;\n  border-radius: 30px;\n\n  box-shadow: inset 8px 8px 15px #e4e4e4, inset -8px -8px 15px #ffffff;\n}\n\nh1 {\n  margin-top: 30px;\n}\n\n.neu-border {\n  height: 500px;\n  border-radius: 30px;\n  background: #f5f5f5;\n  box-shadow: 8px 8px 15px #e4e4e4, -8px -8px 15px #ffffff;\n}\n.neu-border-inset {\n  border-radius: 30px;\n  background: #f5f5f5;\n  box-shadow: inset 8px 8px 15px #e4e4e4, inset -8px -8px 15px #ffffff;\n}\n\n.form {\n  margin: auto;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  padding: 40px;\n  gap: 20px;\n  width: 100%;\n  margin: auto;\n  max-width: 600px;\n}\n\n.form-heading {\n  text-align: center;\n  text-transform: uppercase;\n}\n\n.form-input,\n.form-btn {\n  height: 70px;\n  border: none;\n  outline: none;\n  padding: 20px;\n}\n\n.form-btn {\n  height: 60px;\n  margin-left:5px;\n  width: 150px;\n  cursor: pointer;\n  transition: all 0.1s linear;\n}\n\n.form-btn:hover {\n  transform: scale(1.05);\n}\n</style>\n"]}]}